
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Region</th>
            <th>Type of milk</th>
            <th>Rating</th>
        </tr>
    </thead>
    <tbody>
    {% for cheese in cheeses %}
        <tr data-url="{{ path('app_cheeses_show', {'name': cheese.name }) }}">
            <td><a href="{{ path('app_cheeses_show', {'name': cheese.name }) }}">{{ cheese.name }}</a></td>
            <td>{{ cheese.region }}</td>
            <td>{{ cheese.milk }}</td>
            <td>{% for star in range(1, cheese.score) %}<span>â˜…</span>{% endfor %}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
