{% extends 'App::base.html.twig' %}

{% block body %}

<h1>{{ cheese.name }}</h1>
<em><a href="{{ path('index_cheeses_milk', {'milk': cheese.milk}) }}">{{ cheese.milk }}</a> cheese from <a href="{{ path('index_cheeses_region', {'region': cheese.region}) }}">{{ cheese.region }}</a></em>

<p>Rating: {{ cheese.score }}</p>

<footer>
    <a href="{{ path('index_cheeses') }}">Back</a>
</footer>

{% endblock %}
