<thead>
    {% for id, title in elements|toHeadersArray(options) %}
        {% block thead %}<th id="header-{{ id }}">{{ title }}</th>{% endblock %}
    {% endfor %}
</thead>
<tbody>
    {% for element in elements %}
        {{ bootstrap_datatable_row(element, elements|toHeadersArray(options), options) }}
    {% endfor %}
</tbody>